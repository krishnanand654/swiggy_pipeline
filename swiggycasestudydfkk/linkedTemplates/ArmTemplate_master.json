{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "swiggycasestudydfkk"
		},
		"AzureDataLakeStorage1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage1'"
		},
		"AzureDatabricks1_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'AzureDatabricks1'"
		},
		"sqlconnector_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'sqlconnector'"
		},
		"AzureDataLakeStorage1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://swiggycasestudykk.dfs.core.windows.net/"
		},
		"AzureDatabricks1_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "0101-123724-ug9v9wak"
		},
		"fromgithub_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://github.com/Jr-Newt/swiggy_dataset/raw/refs/heads/main/data.json"
		},
		"sqlconnector_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "swiggy-dev-centralus.database.windows.net"
		},
		"sqlconnector_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "swiggy_db"
		},
		"sqlconnector_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "swiggy_dev_admin"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "swiggycasestudydfkk_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStorage1_accountKey": {
						"value": "[parameters('AzureDataLakeStorage1_accountKey')]"
					},
					"AzureDatabricks1_accessToken": {
						"value": "[parameters('AzureDatabricks1_accessToken')]"
					},
					"sqlconnector_password": {
						"value": "[parameters('sqlconnector_password')]"
					},
					"AzureDataLakeStorage1_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage1_properties_typeProperties_url')]"
					},
					"AzureDatabricks1_properties_typeProperties_existingClusterId": {
						"value": "[parameters('AzureDatabricks1_properties_typeProperties_existingClusterId')]"
					},
					"fromgithub_properties_typeProperties_url": {
						"value": "[parameters('fromgithub_properties_typeProperties_url')]"
					},
					"sqlconnector_properties_typeProperties_server": {
						"value": "[parameters('sqlconnector_properties_typeProperties_server')]"
					},
					"sqlconnector_properties_typeProperties_database": {
						"value": "[parameters('sqlconnector_properties_typeProperties_database')]"
					},
					"sqlconnector_properties_typeProperties_userName": {
						"value": "[parameters('sqlconnector_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}