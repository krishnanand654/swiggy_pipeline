{
	"name": "Archive",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "main_archive_source",
						"type": "DatasetReference"
					},
					"name": "datalakerawdata"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "archive_sink",
						"type": "DatasetReference"
					},
					"name": "ToArchiveFolder",
					"rejectedDataLinkedService": {
						"referenceName": "AzureDataLakeStorage1",
						"type": "LinkedServiceReference"
					}
				}
			],
			"transformations": [],
			"scriptLines": [
				"source(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     purgeFiles: true,",
				"     documentForm: 'documentPerLine') ~> datalakerawdata",
				"datalakerawdata sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     format: 'parquet',",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     outputAssertFailedRows: true,",
				"     assertFailure_fileSystem: 'swiggydataset',",
				"     assertFailure_folderPath: 'Error_Log',",
				"     partitionBy('roundRobin', 5)) ~> ToArchiveFolder"
			]
		}
	}
}